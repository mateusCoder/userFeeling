import java.util.Scanner;
import java.util.regex.Matcher;
import java.util.regex.Pattern;

public class userFeelingApplication {

	public static void main(String[] args) {
		
		String text;
		int happyCount = 0;
		int sadCount = 0;
		
		Scanner read = new Scanner(System.in);
		
		System.out.println("+------- MEDIDOR DE SENTIMENTO COMPASS -------+ \n");
		System.out.println("Digite uma frase: ");
		text = (read.next());
		
		text = "Sonhei com a prova :-( (vou estudar)";
		text = "Espero que esteja tudo bem:-)";
		
		Pattern happyFace = Pattern.compile(":-\\)");  
		Matcher happy = happyFace.matcher(text); 
		
		Pattern sadFace = Pattern.compile(":-\\("); 
		Matcher sad = sadFace.matcher(text);  

		while(happy.find()){
		     happyCount++;		
		}

		while(sad.find()){
		     sadCount++;		
		}
		
		System.out.println(happyCount);
		System.out.println(sadCount);
		
		if(happyCount > sadCount){ 
			System.out.println("divertido");
		}else if(sadCount > happyCount) {
			System.out.println("chateado");
		}else {
			System.out.println("neutro");
		}
	}
}
